pipeline {
    agent any

    stages {
        stage("clone the repo") {
            steps {
                echo 'clone the repo'
            }
        }        
        stage("Build") {
            steps {
                echo 'build the code'
            }
        }
        stage("Test") {
            steps {
                echo 'quality checks'
            }        
        }
        stage("Deploy") {
            steps {
                echo 'deploy the code'
            }        
        }
    }
}
